job('SimpleDSLGit'){
	scm {
		git('https://github.com/andreiribo/myRepository.git')
	}
	parameters {
		stringParam( 'Name' , 'DefaultName' , 'FirstParam')
	}
	steps{
		shell('echo $JOB_NAME')
		downstreamParameterized {
			trigger('ParamJob2') {
				parameters {
					peredefinedProp('Name' , '$Name')
				}
			}
		}
	}
}
